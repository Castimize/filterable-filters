{
    "name": "tomodo531/filterable-filters",
    "description": "A Laravel Nova filter.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Tomodo531\\FilterableFilters\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tomodo531\\FilterableFilters\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
